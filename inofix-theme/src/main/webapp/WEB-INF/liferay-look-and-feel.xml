<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" 
    "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">
<!-- 
    liferay-look-and-feel.xml:  Look and feel configuration of Inofix' 
    default theme.
    
    Created:    2015-07-26 16:27 by Christian Berndt
    Modified:   2015-08-02 13:20 by Christian Berndt
    Version:    1.0.1   
 -->
<look-and-feel>
    <compatibility>
        <version>6.2.3+</version>
    </compatibility>
    <theme id="inofix" name="inofix-theme">
        <settings>
            <setting key="brand" configurable="true" type="text"
                value="Your Brand" />
            <setting key="default-font" configurable="true"
                type="text" value="https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,600" />
            <setting key="portlet-setup-show-borders-default"
                value="false" />
        </settings>
    </theme>    
</look-and-feel>